[32m+[m[32mimport CalculatorElement from "./calculatorElement";[m
[32m+[m
[32m+[m[32mexport default class ConsumerCreditCalculator extends CalculatorElement {[m
[32m+[m[32m  constructor() {[m
[32m+[m[32m    super();[m
[32m+[m[32m    this.purpose = "consumer-credit";[m
[32m+[m[32m    this.minPrice = 800000;[m
[32m+[m[32m    this.maxPrice = 5000000;[m
[32m+[m[32m    this.priceStep = 50000;[m
[32m+[m[32m    this.minInitialFeePercantage = 20;[m
[32m+[m[32m    this.maxInitialFeePercantage = 100;[m
[32m+[m[32m    this.initialFeeStepPercantage = 5;[m
[32m+[m[32m    this.minCreditSum = 0;[m
[32m+[m[32m    this.minTerm = 1;[m
[32m+[m[32m    this.maxTerm = 7;[m
[32m+[m[32m    this.bankMember = false;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  get interestRate() {[m
[32m+[m[32m    let interestRate;[m
[32m+[m
[32m+[m[32m    if (this.price < 750000) {[m
[32m+[m[32m      interestRate = 15;[m
[32m+[m[32m    } else if (this.price >= 750000 && this.price < 2000000) {[m
[32m+[m[32m      interestRate = 12.5;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      interestRate = 9.5;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (this.bankMember) {[m
[32m+[m[32m      interestRate -= 0.5;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return interestRate;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  get decreaseSum() {[m
[32m+[m[32m    return 0;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  connectedCallback() {[m
[32m+[m[32m    super.connectedCallback();[m
[32m+[m[32m    this.querySelector(".calculator__extra-options").append([m
[32m+[m[32m      document[m
[32m+[m[32m        .querySelector(".calculator__template-checkbox_consumer-credit")[m
[32m+[m[32m        .content.cloneNode(true)[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    this.querySelector(".calculator__input-container_initial-fee").remove();[m
[32m+[m
[32m+[m[32m    this.querySelector(".input-checkbox_bank-member").addEventListener([m
[32m+[m[32m      "change",[m
[32m+[m[32m      () => {[m
[32m+[m[32m        this.bankMember = event.currentTarget.checked;[m
[32m+[m[32m        this.updateForm();[m
[32m+[m[32m      }[m
[32m+[m[32m    );[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
