[32m+[m[32m  function showOffer([m
[32m+[m[32m    creditSum,[m
[32m+[m[32m    monthlyPayment,[m
[32m+[m[32m    interestRate,[m
[32m+[m[32m    requiredIncome,[m
[32m+[m[32m    purpose,[m
[32m+[m[32m    minCreditSum[m
[32m+[m[32m  ) {[m
[32m+[m[32m    const offer = document.querySelector(".offer");[m
[32m+[m[32m    const creditSumOutput = document.querySelector(".offer__value_credit-sum");[m
[32m+[m[32m    const monthlyPaymentOutput = document.querySelector([m
[32m+[m[32m      ".offer__value_monthly-payment"[m
[32m+[m[32m    );[m
[32m+[m[32m    const interestRateOutput = document.querySelector([m
[32m+[m[32m      ".offer__value_interest-rate"[m
[32m+[m[32m    );[m
[32m+[m[32m    const requiredIncomeOutput = document.querySelector([m
[32m+[m[32m      ".offer__value_required-income"[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    creditSumOutput.textContent = creditSum;[m
[32m+[m[32m    monthlyPaymentOutput.textContent = monthlyPayment;[m
[32m+[m[32m    interestRateOutput.textContent = interestRate;[m
[32m+[m[32m    requiredIncomeOutput.textContent = requiredIncome;[m
[32m+[m
[32m+[m[32m    let title;[m
[32m+[m[32m    let priceLabel;[m
[32m+[m[32m    let creditSumLabel;[m
[32m+[m
[32m+[m[32m    if (purpose === "mortgage") {[m
[32m+[m[32m      title = `Наш банк не выдаёт ипотечные кредиты меньше ${minCreditSum} рублей.`;[m
[32m+[m[32m      priceLabel = "Стоимость недвижимости";[m
[32m+[m[32m      creditSumLabel = "Сумма ипотеки";[m
[32m+[m[32m    } else if (purpose === "car-credit") {[m
[32m+[m[32m      title = `Наш банк не выдаёт автокредиты меньше ${minCreditSum} рублей.`;[m
[32m+[m[32m      priceLabel = "Стоимость автомобиля";[m
[32m+[m[32m      creditSumLabel = "Сумма автокредита";[m
[32m+[m[32m    } else if (purpose === "consumer-credit") {[m
[32m+[m[32m      title = "";[m
[32m+[m[32m      priceLabel = "Сумма потребительского кредита";[m
[32m+[m[32m      creditSumLabel = "Сумма кредита";[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    document.querySelector(".refuse__title").textContent = title;[m
[32m+[m[32m    document.querySelector(".calculator__label_price").textContent = priceLabel;[m
[32m+[m[32m    document.querySelector([m
[32m+[m[32m      ".offer__label_credit-sum"[m
[32m+[m[32m    ).textContent = creditSumLabel;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function makeRequest(purpose, price, initialFee, term) {[m
[32m+[m[32m    const request = document.querySelector(".request");[m
[32m+[m[32m    const requestForm = document.querySelector(".request__form");[m
[32m+[m[32m    const inputRequestNumber = request.querySelector([m
[32m+[m[32m      ".request__input_request-number"[m
[32m+[m[32m    );[m
[32m+[m[32m    const inputPurpose = request.querySelector(".request__input_purpose");[m
[32m+[m[32m    const inputPrice = request.querySelector(".request__input_price");[m
[32m+[m[32m    const inputInitialFee = request.querySelector(".request__input_initial-fee");[m
[32m+[m[32m    const inputTerm = request.querySelector(".request__input_term");[m
[32m+[m[32m    const labelPrice = request.querySelector(".request__label_price");[m
[32m+[m[32m    const userInfoInputs = request.querySelectorAll(".request__input");[m
[32m+[m[32m    const inputFullname = request.querySelector(".request__input_fullname");[m
[32m+[m[32m    const inputTelephone = request.querySelector(".request__input_phone-number");[m
[32m+[m[32m    const inputEmail = request.querySelector(".request__input_email");[m
[32m+[m
[32m+[m[32m    inputFullname.focus();[m
[32m+[m[32m    userInfoInputs.forEach((item) => {[m
[32m+[m[32m      item.addEventListener("change", () => checkEmpty(item));[m
[32m+[m[32m    });[m
[32m+[m[32m    requestForm.addEventListener("submit", submitForm);[m
[32m+[m
[32m+[m[32m    function checkEmpty(input) {[m
[32m+[m[32m      input.nextElementSibling;[m
[32m+[m[32m      if (!input.value.length) {[m
[32m+[m[32m        input.nextElementSibling.classList.remove("request__label_moved");[m
[32m+[m[32m      } else {[m
[32m+[m[32m        input.nextElementSibling.classList.add("request__label_moved");[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (localStorage.getItem("user-data") === null) {[m
[32m+[m[32m      localStorage.setItem("user-data", []);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    //slice(-4) добавляет числу лишние нули[m
[32m+[m[32m    inputRequestNumber.value = `№ ${([m
[32m+[m[32m    "0000" +[m
[32m+[m[32m    (+localStorage.getItem("requestsNumber") + 1)[m
[32m+[m[32m  ).slice(-4)}`;[m
[32m+[m
[32m+[m[32m    function submitForm() {[m
[32m+[m[32m      event.preventDefault();[m
[32m+[m
[32m+[m[32m      if (!isFormVaild()) {[m
[32m+[m[32m        request.classList.add("request_invalid");[m
[32m+[m
[32m+[m[32m        request.addEventListener("animationend", removeAnimation);[m
[32m+[m
[32m+[m[32m        function removeAnimation() {[m
[32m+[m[32m          request.classList.remove("request_invalid");[m
[32m+[m[32m          request.removeEventListener("animationend", removeAnimation);[m
[32m+[m[32m        }[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      localStorage.setItem([m
[32m+[m[32m        "requestsNumber",[m
[32m+[m[32m        +localStorage.getItem("requestsNumber") + 1[m
[32m+[m[32m      );[m
[32m+[m
[32m+[m[32m      localStorage.getItem("user-data");[m
[32m+[m
[32m+[m[32m      localStorage.setItem("user-data", [[m
[32m+[m[32m        //...localStorage.getItem("user-data"),[m
[32m+[m[32m        {[m
[32m+[m[32m          fullName: inputFullname.value,[m
[32m+[m[32m          telephone: inputTelephone.value,[m
[32m+[m[32m          email: inputEmail.value,[m
[32m+[m[32m          number: +localStorage.getItem("requestsNumber") + 1,[m
[32m+[m[32m        },[m
[32m+[m[32m      ]);[m
[32m+[m
[32m+[m[32m      inputFullname.value = "";[m
[32m+[m[32m      inputTelephone.value = "";[m
[32m+[m[32m      inputEmail.value = "";[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function isFormVaild() {[m
[32m+[m[32m      if ([m
[32m+[m[32m        inputFullname.value.length &&[m
[32m+[m[32m        inputTelephone.value.length &&[m
[32m+[m[32m        inputEmail.value.length[m
[32m+[m[32m      ) {[m
[32m+[m[32m        return true;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        return false;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    request.classList.remove("visually-hidden");[m
[32m+[m
[32m+[m[32m    if (purpose === "mortgage") {[m
[32m+[m[32m      inputPurpose.value = "Ипотека";[m
[32m+[m[32m      labelPrice.textContent = "Стоимость недвижимости";[m
[32m+[m[32m    } else if (purpose === "car-credit") {[m
[32m+[m[32m      inputPurpose.value = "Автокредит";[m
[32m+[m[32m      labelPrice.textContent = "Стоимость автомобиля";[m
[32m+[m[32m    } else {[m
[32m+[m[32m      inputPurpose.value = "Потребительский кредит";[m
[32m+[m[32m      labelPrice.textContent = "Сумма кредита";[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    inputPrice.value = price;[m
[32m+[m[32m    inputInitialFee.value = initialFee;[m
[32m+[m[32m    inputTerm.value = term;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const ERROR_MESSAGE = `Некорректное значение`;[m
[32m+[m
[32m+[m[32m  class CalculatorElement extends HTMLElement {[m
[32m+[m[32m    constructor(defaultPrice, defaultTerm) {[m
[32m+[m[32m      super();[m
[32m+[m[32m      this.price = 0;[m
[32m+[m[32m      this.initialFee = 0;[m
[32m+[m[32m      this.initialFeePercantage = 0;[m
[32m+[m[32m      this.creditSum = 0;[m
[32m+[m[32m      this.term = 0;[m
[32m+[m[32m      this.monthlyPayment = 0;[m
[32m+[m[32m      this.requiredIncome = 0;[m
[32m+[m[32m      this.requiredIncomCoefficient = 45;[m
[32m+[m[32m      this.cursorPositionInputPrice = null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    updateForm() {[m
[32m+[m[32m      this.inputPrice.value = this.transformValueToString(this.price, [[m
[32m+[m[32m        "рубль",[m
[32m+[m[32m        "рубля",[m
[32m+[m[32m        "рублей",[m
[32m+[m[32m      ]);[m
[32m+[m
[32m+[m[32m      this.inputPrice.selectionStart =[m
[32m+[m[32m        this.inputPrice.value.length - this.cursorPositionInputPrice;[m
[32m+[m[32m      this.inputPrice.selectionEnd = this.inputPrice.selectionStart;[m
[32m+[m
[32m+[m[32m      this.inputInitialFee.value = this.transformValueToString(this.initialFee, [[m
[32m+[m[32m        "рубль",[m
[32m+[m[32m        "рубля",[m
[32m+[m[32m        "рублей",[m
[32m+[m[32m      ]);[m
[32m+[m
[32m+[m[32m      this.rangeInitialFee.value = this.initialFeePercantage;[m
[32m+[m[32m      this.outputInitialFee.value = `${this.initialFeePercantage}%`;[m
[32m+[m
[32m+[m[32m      this.inputTerm.value = this.transformValueToString(this.term, [[m
[32m+[m[32m        "год",[m
[32m+[m[32m        "года",[m
[32m+[m[32m        "лет",[m
[32m+[m[32m      ]);[m
[32m+[m
[32m+[m[32m      this.rangeTerm.value = this.term;[m
[32m+[m
[32m+[m[32m      this.updateOffer();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    updateOffer() {[m
[32m+[m[32m      this.creditSum = this.price - this.initialFee - this.decreaseSum;[m
[32m+[m
[32m+[m[32m      if (this.creditSum < this.minCreditSum) {[m
[32m+[m[32m        document.querySelector(".offer").classList.add("visually-hidden");[m
[32m+[m[32m        document.querySelector(".refuse").classList.remove("visually-hidden");[m
[32m+[m[32m        return;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        document.querySelector(".offer").classList.remove("visually-hidden");[m
[32m+[m[32m        document.querySelector(".refuse").classList.add("visually-hidden");[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      let monthlyInterestRate = this.interestRate / 12 / 100;[m
[32m+[m
[32m+[m[32m      this.monthlyPayment = Math.ceil([m
[32m+[m[32m        this.creditSum *[m
[32m+[m[32m          (monthlyInterestRate +[m
[32m+[m[32m            monthlyInterestRate /[m
[32m+[m[32m              ((1 + monthlyInterestRate) ** (this.term * 12) - 1))[m
[32m+[m[32m      );[m
[32m+[m
[32m+[m[32m      this.requiredIncome = Math.ceil([m
[32m+[m[32m        (this.monthlyPayment * 100) / this.requiredIncomCoefficient[m
[32m+[m[32m      );[m
[32m+[m
[32m+[m[32m      showOffer([m
[32m+[m[32m        this.transformValueToString(this.creditSum, ["рубль", "рубля", "рублей"]),[m
[32m+[m[32m        this.transformValueToString(this.monthlyPayment, [[m
[32m+[m[32m          "рубль",[m
[32m+[m[32m          "рубля",[m
[32m+[m[32m          "рублей",[m
[32m+[m[32m        ]),[m
[32m+[m[32m        `${this.transformValueToString([m
[32m+[m[32m        this.interestRate.toFixed(2).replace(/\./, ",")[m
[32m+[m[32m      )}%`,[m
[32m+[m[32m        this.transformValueToString(this.requiredIncome, [[m
[32m+[m[32m          "рубль",[m
[32m+[m[32m          "рубля",[m
[32m+[m[32m          "рублей",[m
[32m+[m[32m        ]),[m
[32m+[m[32m        this.purpose,[m
[32m+[m[32m        this.minCreditSum[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    updatePrice(priceValue) {[m
[32m+[m[32m      if (priceValue) {[m
[32m+[m[32m        this.price += priceValue;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.saveCursorPosition(this.inputPrice, "cursorPositionInputPrice");[m
[32m+[m
[32m+[m[32m        this.digitsPattern(this.inputPrice);[m
[32m+[m[32m        this.price = this.transformStringToNumber(this.inputPrice.value);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.checkValidPrice();[m
[32m+[m[32m      this.calculateMinInitialFee();[m
[32m+[m[32m      this.updateForm();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    updateInitialFee() {[m
[32m+[m[32m      this.digitsPattern(this.inputInitialFee);[m
[32m+[m[32m      this.saveCursorPosition([m
[32m+[m[32m        this.inputInitialFee,[m
[32m+[m[32m        "cursorPositionInputInitialFee"[m
[32m+[m[32m      );[m
[32m+[m[32m      this.initialFee = Math.ceil([m
[32m+[m[32m        this.transformStringToNumber(this.inputInitialFee.value)[m
[32m+[m[32m      );[m
[32m+[m[32m      this.initialFeePercantage = this.calculateInitialFeePercantage();[m
[32m+[m
[32m+[m[32m      this.updateForm();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    updateInitialFeePercantage() {[m
[32m+[m[32m      this.initialFeePercantage = this.rangeInitialFee.value;[m
[32m+[m[32m      this.initialFee = Math.ceil((this.initialFeePercantage * this.price) / 100);[m
[32m+[m
[32m+[m[32m      this.updateForm();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    updateTerm(termInput) {[m
[32m+[m[32m      this.digitsPattern(termInput);[m
[32m+[m[32m      this.saveCursorPosition(this.inputTerm, "cursorPositionInputTerm");[m
[32m+[m[32m      this.term = this.transformStringToNumber(termInput.value);[m
[32m+[m
[32m+[m[32m      this.updateForm();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    saveCursorPosition(input, cursorName) {[m
[32m+[m[32m      if (/[0-9]/.test(input.value[input.selectionStart - 1])) {[m
[32m+[m[32m        this[cursorName] = input.value.length - input.selectionStart;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this[cursorName] = input.value.length - input.selectionStart + 1;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    checkValidPrice() {[m
[32m+[m[32m      if (this.price > this.maxPrice || this.price < this.minPrice) {[m
[32m+[m[32m        this.inputPrice.classList.add("calculator__input_price_invalid");[m
[32m+[m[32m        document.querySelector([m
[32m+[m[32m          ".calculator__invalid-input-message"[m
[32m+[m[32m        ).textContent = ERROR_MESSAGE;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.inputPrice.classList.remove("calculator__input_price_invalid");[m
[32m+[m[32m        document.querySelector(".calculator__invalid-input-message").textContent =[m
[32m+[m[32m          "";[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    checkValidInitialFee() {[m
[32m+[m[32m      if (this.initialFeePercantage < this.minInitialFeePercantage) {[m
[32m+[m[32m        this.initialFee = this.price / this.minInitialFeePercantage;[m
[32m+[m[32m        this.initialFeePercantage = this.minInitialFeePercantage;[m
[32m+[m[32m        this.updateForm();[m
[32m+[m[32m      } else if (this.initialFee > this.price) {[m
[32m+[m[32m        this.initialFee = this.price;[m
[32m+[m[32m        this.initialFeePercantage = this.maxInitialFeePercantage;[m
[32m+[m[32m        this.updateForm();[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    checkValidTerm() {[m
[32m+[m[32m      if (this.term > this.maxTerm) {[m
[32m+[m[32m        this.term = this.maxTerm;[m
[32m+[m[32m        this.updateForm();[m
[32m+[m[32m      } else if (this.term < this.maxTerm) {[m
[32m+[m[32m        this.term = this.minTerm;[m
[32m+[m[32m        this.updateForm();[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    calculateMinInitialFee() {[m
[32m+[m[32m      this.initialFee = Math.ceil([m
[32m+[m[32m        (this.price / 100) * this.minInitialFeePercantage[m
[32m+[m[32m      );[m
[32m+[m[32m      this.initialFeePercantage = this.calculateInitialFeePercantage();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    calculateInitialFeePercantage() {[m
[32m+[m[32m      return Math.round((this.initialFee / this.price) * 100);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    transformValueToString(number, units) {[m
[32m+[m[32m      let unit;[m
[32m+[m[32m      if (units) {[m
[32m+[m[32m        unit = ` ${this.plural(number, units)}`;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        unit = "";[m
[32m+[m[32m      }[m
[32m+[m[32m      return `${number[m
[32m+[m[32m      .toString()[m
[32m+[m[32m      .replace(/(\d)(?=(\d{3})+([^\d]|$))/g, "$1 ")}${unit}`;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    digitsPattern(input) {[m
[32m+[m[32m      input.value = input.value.replace(/[^0-9 ]/, "");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    transformStringToNumber(str) {[m
[32m+[m[32m      return +str.replace(/\D/g, "");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    plural(number, units) {[m
[32m+[m[32m      number = Math.abs(number);[m
[32m+[m[32m      number %= 100;[m
[32m+[m[32m      if (number >= 5 && number <= 20) {[m
[32m+[m[32m        return units[2];[m
[32m+[m[32m      }[m
[32m+[m[32m      number %= 10;[m
[32m+[m[32m      if (number === 1) {[m
[32m+[m[32m        return units[0];[m
[32m+[m[32m      }[m
[32m+[m[32m      if (number >= 2 && number <= 4) {[m
[32m+[m[32m        return units[1];[m
[32m+[m[32m      }[m
[32m+[m[32m      return units[2];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    connectedCallback() {[m
[32m+[m[32m      this.append([m
[32m+[m[32m        document.querySelector(".calculator-template").content.cloneNode(true)[m
[32m+[m[32m      );[m
[32m+[m
[32m+[m[32m      this.inputPrice = this.querySelector(".calculator__input_price");[m
[32m+[m[32m      this.inputInitialFee = this.querySelector(".calculator__input_initial-fee");[m
[32m+[m[32m      this.rangeInitialFee = this.querySelector([m
[32m+[m[32m        ".calculator__input-range_initial-fee"[m
[32m+[m[32m      );[m
[32m+[m[32m      this.outputInitialFee = this.querySelector([m
[32m+[m[32m        ".calculator__range-extra-text_initial-fee-output"[m
[32m+[m[32m      );[m
[32m+[m[32m      this.inputTerm = this.querySelector(".calculator__input_term");[m
[32m+[m[32m      this.rangeTerm = this.querySelector(".calculator__range_term");[m
[32m+[m[32m      this.querySelector(".calculator__range-extra-text__min").textContent = `${[m
[32m+[m[32m      this.minTerm[m
[32m+[m[32m    } ${this.plural(this.minTerm, ["год", "лет", "лет"])}`;[m
[32m+[m[32m      this.querySelector(".calculator__range-extra-text__max").textContent = `${[m
[32m+[m[32m      this.maxTerm[m
[32m+[m[32m    } ${this.plural(this.maxTerm, ["год", "лет", "лет"])}`;[m
[32m+[m[32m      this.priceLabel = this.querySelector(".calculator__range-extra-text_price");[m
[32m+[m
[32m+[m[32m      this.priceLabel.textContent = `От ${this.transformValueToString([m
[32m+[m[32m      this.minPrice[m
[32m+[m[32m    )} до ${this.transformValueToString(this.maxPrice)} рублей`;[m
[32m+[m
[32m+[m[32m      this.inputPrice.min = this.minPrice;[m
[32m+[m[32m      this.inputPrice.max = this.maxPrice;[m
[32m+[m[32m      this.inputPrice.step = this.priceStep;[m
[32m+[m
[32m+[m[32m      this.rangeInitialFee.min = this.minInitialFeePercantage;[m
[32m+[m[32m      this.rangeInitialFee.max = 100;[m
[32m+[m[32m      this.rangeInitialFee.step = this.initialFeeStepPercantage;[m
[32m+[m
[32m+[m[32m      this.inputTerm.min = this.minTerm;[m
[32m+[m[32m      this.inputTerm.max = this.maxTerm;[m
[32m+[m[32m      this.rangeTerm.min = this.minTerm;[m
[32m+[m[32m      this.rangeTerm.max = this.maxTerm;[m
[32m+[m
[32m+[m[32m      this.price = this.minPrice;[m
[32m+[m[32m      this.term = this.minTerm;[m
[32m+[m
[32m+[m[32m      this.calculateMinInitialFee();[m
[32m+[m[32m      this.updateForm();[m
[32m+[m
[32m+[m[32m      this.inputPrice.addEventListener("input", () => this.updatePrice());[m
[32m+[m[32m      this.inputInitialFee.addEventListener("input", () =>[m
[32m+[m[32m        this.updateInitialFee()[m
[32m+[m[32m      );[m
[32m+[m[32m      this.inputInitialFee.addEventListener("change", () =>[m
[32m+[m[32m        this.checkValidInitialFee()[m
[32m+[m[32m      );[m
[32m+[m[32m      this.inputInitialFee.addEventListener("input", () =>[m
[32m+[m[32m        this.updateInitialFee()[m
[32m+[m[32m      );[m
[32m+[m[32m      this.rangeInitialFee.addEventListener("input", () =>[m
[32m+[m[32m        this.updateInitialFeePercantage()[m
[32m+[m[32m      );[m
[32m+[m[32m      this.inputTerm.addEventListener("input", () =>[m
[32m+[m[32m        this.updateTerm(this.inputTerm)[m
[32m+[m[32m      );[m
[32m+[m[32m      this.rangeTerm.addEventListener("input", () =>[m
[32m+[m[32m        this.updateTerm(this.rangeTerm)[m
[32m+[m[32m      );[m
[32m+[m[32m      this.inputTerm.addEventListener("change", () => this.checkValidTerm());[m
[32m+[m[32m      this.querySelector(".calculator__step-up").addEventListener("click", () => {[m
[32m+[m[32m        this.updatePrice(this.priceStep);[m
[32m+[m[32m      });[m
[32m+[m[32m      this.querySelector(".calculator__step-down").addEventListener([m
[32m+[m[32m        "click",[m
[32m+[m[32m        () => {[m
[32m+[m[32m          this.updatePrice(-this.priceStep);[m
[32m+[m[32m        }[m
[32m+[m[32m      );[m
[32m+[m[32m      document[m
[32m+[m[32m        .querySelector(".offer__button")[m
[32m+[m[32m        .addEventListener("click", () =>[m
[32m+[m[32m          makeRequest([m
[32m+[m[32m            this.transformValueToString(this.purpose),[m
[32m+[m[32m            this.transformValueToString(this.price, ["рубль", "рубля", "рублей"]),[m
[32m+[m[32m            this.transformValueToString(this.initialFee, [[m
[32m+[m[32m              "рубль",[m
[32m+[m[32m              "рубля",[m
[32m+[m[32m              "рублей",[m
[32m+[m[32m            ]),[m
[32m+[m[32m            this.transformValueToString(this.term, ["год", "лет", "лет"])[m
[32m+[m[32m          )[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  class MortgageCalculator extends CalculatorElement {[m
[32m+[m[32m    constructor() {[m
[32m+[m[32m      super();[m
[32m+[m[32m      this.purpose = "mortgage";[m
[32m+[m[32m      this.minPrice = 1200000;[m
[32m+[m[32m      this.maxPrice = 25000000;[m
[32m+[m[32m      this.priceStep = 100000;[m
[32m+[m[32m      this.minInitialFeePercantage = 10;[m
[32m+[m[32m      this.maxInitialFeePercantage = 100;[m
[32m+[m[32m      this.initialFeeStepPercantage = 5;[m
[32m+[m[32m      this.minCreditSum = 500000;[m
[32m+[m[32m      this.minTerm = 5;[m
[32m+[m[32m      this.maxTerm = 30;[m
[32m+[m[32m      this.maternalCapitalSum = 470000;[m
[32m+[m[32m      this.useMaternalCapital = false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    get interestRate() {[m
[32m+[m[32m      if (this.initialFeePercantage < 15) {[m
[32m+[m[32m        return 9.4;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        return 8.5;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    get decreaseSum() {[m
[32m+[m[32m      return this.useMaternalCapital ? this.maternalCapitalSum : 0;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    connectedCallback() {[m
[32m+[m[32m      super.connectedCallback();[m
[32m+[m[32m      this.querySelector(".calculator__extra-options").append([m
[32m+[m[32m        document[m
[32m+[m[32m          .querySelector(".calculator__template-checkbox_mortgage")[m
[32m+[m[32m          .content.cloneNode(true)[m
[32m+[m[32m      );[m
[32m+[m
[32m+[m[32m      this.querySelector(".input-checkbox").addEventListener("change", () => {[m
[32m+[m[32m        this.useMaternalCapital = event.currentTarget.checked;[m
[32m+[m[32m        this.updateForm();[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  class ConsumerCreditCalculator extends CalculatorElement {[m
[32m+[m[32m    constructor() {[m
[32m+[m[32m      super();[m
[32m+[m[32m      this.purpose = "consumer-credit";[m
[32m+[m[32m      this.minPrice = 800000;[m
[32m+[m[32m      this.maxPrice = 5000000;[m
[32m+[m[32m      this.priceStep = 50000;[m
[32m+[m[32m      this.minInitialFeePercantage = 20;[m
[32m+[m[32m      this.maxInitialFeePercantage = 100;[m
[32m+[m[32m      this.initialFeeStepPercantage = 5;[m
[32m+[m[32m      this.minCreditSum = 0;[m
[32m+[m[32m      this.minTerm = 1;[m
[32m+[m[32m      this.maxTerm = 7;[m
[32m+[m[32m      this.bankMember = false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    get interestRate() {[m
[32m+[m[32m      let interestRate;[m
[32m+[m
[32m+[m[32m      if (this.price < 750000) {[m
[32m+[m[32m        interestRate = 15;[m
[32m+[m[32m      } else if (this.price >= 750000 && this.price < 2000000) {[m
[32m+[m[32m        interestRate = 12.5;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        interestRate = 9.5;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (this.bankMember) {[m
[32m+[m[32m        interestRate -= 0.5;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return interestRate;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    get decreaseSum() {[m
[32m+[m[32m      return 0;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    connectedCallback() {[m
[32m+[m[32m      super.connectedCallback();[m
[32m+[m[32m      this.querySelector(".calculator__extra-options").append([m
[32m+[m[32m        document[m
[32m+[m[32m          .querySelector(".calculator__template-checkbox_consumer-credit")[m
[32m+[m[32m          .content.cloneNode(true)[m
[32m+[m[32m      );[m
[32m+[m
[32m+[m[32m      this.querySelector(".calculator__input-container_initial-fee").remove();[m
[32m+[m
[32m+[m[32m      this.querySelector(".input-checkbox_bank-member").addEventListener([m
[32m+[m[32m        "change",[m
[32m+[m[32m        () => {[m
[32m+[m[32m          this.bankMember = event.currentTarget.checked;[m
[32m+[m[32m          this.updateForm();[m
[32m+[m[32m        }[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  class CarCreditCalculator extends CalculatorElement {[m
[32m+[m[32m    constructor() {[m
[32m+[m[32m      super();[m
[32m+[m[32m      this.purpose = "car-credit";[m
[32m+[m[32m      this.minPrice = 500000;[m
[32m+[m[32m      this.maxPrice = 5000000;[m
[32m+[m[32m      this.priceStep = 50000;[m
[32m+[m[32m      this.minInitialFeePercantage = 20;[m
[32m+[m[32m      this.maxInitialFeePercantage = 100;[m
[32m+[m[32m      this.initialFeeStepPercantage = 5;[m
[32m+[m[32m      this.minCreditSum = 200000;[m
[32m+[m[32m      this.minTerm = 1;[m
[32m+[m[32m      this.maxTerm = 5;[m
[32m+[m[32m      this.casco = false;[m
[32m+[m[32m      this.lifeInsurance = false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    get interestRate() {[m
[32m+[m[32m      if (this.casco && this.lifeInsurance) {[m
[32m+[m[32m        return 3.5;[m
[32m+[m[32m      } else if (this.casco || this.lifeInsurance) {[m
[32m+[m[32m        return 8.5;[m
[32m+[m[32m      } else if (this.price >= 2000000) {[m
[32m+[m[32m        return 15;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        return 16;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    get decreaseSum() {[m
[32m+[m[32m      return 0;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    connectedCallback() {[m
[32m+[m[32m      super.connectedCallback();[m
[32m+[m[32m      this.querySelector(".calculator__extra-options").append([m
[32m+[m[32m        document[m
[32m+[m[32m          .querySelector(".calculator__template-checkbox_car-credit")[m
[32m+[m[32m          .content.cloneNode(true)[m
[32m+[m[32m      );[m
[32m+[m
[32m+[m[32m      this.querySelector(".input-checkbox_casco").addEventListener([m
[32m+[m[32m        "change",[m
[32m+[m[32m        () => {[m
[32m+[m[32m          this.casco = event.currentTarget.checked;[m
[32m+[m[32m          this.updateForm();[m
[32m+[m[32m        }[m
[32m+[m[32m      );[m
[32m+[m
[32m+[m[32m      this.querySelector(".input-checkbox_life-insurance").addEventListener([m
[32m+[m[32m        "change",[m
[32m+[m[32m        () => {[m
[32m+[m[32m          this.lifeInsurance = event.currentTarget.checked;[m
[32m+[m[32m          this.updateForm();[m
[32m+[m[32m        }[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function CalculatorCustomSelect() {[m
[32m+[m[32m    const select = document.querySelector(".calculator__select");[m
[32m+[m[32m    const selected = select.querySelector(".calculator__selected");[m
[32m+[m[32m    const selectValue = select.querySelector(".calculator__select-value");[m
[32m+[m[32m    const optionsWrap = select.querySelector(".calculator__options-wrap");[m
[32m+[m[32m    const optionsContainer = select.querySelector(".calculator__options");[m
[32m+[m[32m    const options = select.querySelectorAll(".calculator__option");[m
[32m+[m[32m    const customCalculatorContainer = document.querySelector([m
[32m+[m[32m      ".calculator__custom-calculator-container"[m
[32m+[m[32m    );[m
[32m+[m[32m    const offer = document.querySelector(".offer");[m
[32m+[m[32m    const request = document.querySelector(".request");[m
[32m+[m[32m    const optionsOpenedClass = "calculator__options_opened";[m
[32m+[m[32m    const noBottomRadiusClass = "calculator__selected_no-bottom-radius";[m
[32m+[m
[32m+[m[32m    selected.addEventListener("click", toggleOptions);[m
[32m+[m[32m    options.forEach((item) => {[m
[32m+[m[32m      item.addEventListener("click", () => selectOption(item));[m
[32m+[m[32m    });[m
[32m+[m[32m    window.addEventListener("click", checkClockOutside);[m
[32m+[m
[32m+[m[32m    function hideOptions() {[m
[32m+[m[32m      optionsContainer.classList.remove(optionsOpenedClass);[m
[32m+[m[32m      selected.classList.remove(noBottomRadiusClass);[m
[32m+[m[32m      optionsContainer.addEventListener("transitionend", addVisuallyHiddenClass);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function addVisuallyHiddenClass() {[m
[32m+[m[32m      optionsWrap.classList.add("visually-hidden");[m
[32m+[m[32m      optionsContainer.removeEventListener([m
[32m+[m[32m        "transitionend",[m
[32m+[m[32m        addVisuallyHiddenClass[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function checkClockOutside() {[m
[32m+[m[32m      if ([m
[32m+[m[32m        !select.contains(event.target) &&[m
[32m+[m[32m        optionsContainer.classList.contains(optionsOpenedClass)[m
[32m+[m[32m      ) {[m
[32m+[m[32m        hideOptions();[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function toggleOptions() {[m
[32m+[m[32m      if (optionsWrap.classList.contains("visually-hidden")) {[m
[32m+[m[32m        optionsWrap.classList.remove("visually-hidden");[m
[32m+[m[32m        optionsContainer.classList.add(optionsOpenedClass);[m
[32m+[m[32m        selected.classList.add(noBottomRadiusClass);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        hideOptions();[m
[32m+[m[32m        changeCalculator();[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function selectOption(option) {[m
[32m+[m[32m      select.querySelector(".calculator__option_selected") &&[m
[32m+[m[32m        select[m
[32m+[m[32m          .querySelector(".calculator__option_selected")[m
[32m+[m[32m          .classList.remove("calculator__option_selected");[m
[32m+[m
[32m+[m[32m      option.classList.add("calculator__option_selected");[m
[32m+[m[32m      selectValue.textContent = option.textContent;[m
[32m+[m[32m      optionsContainer.classList.remove(optionsOpenedClass);[m
[32m+[m[32m      selected.classList.remove(noBottomRadiusClass);[m
[32m+[m[32m      hideOptions();[m
[32m+[m
[32m+[m[32m      changeCalculator(option.dataset.value);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function changeCalculator(calculatorName) {[m
[32m+[m[32m      let calculator;[m
[32m+[m
[32m+[m[32m      customCalculatorContainer.firstElementChild &&[m
[32m+[m[32m        customCalculatorContainer.firstElementChild.remove();[m
[32m+[m
[32m+[m[32m      if (calculatorName === "mortgage") {[m
[32m+[m[32m        calculator = document.createElement("mortgage-calculator");[m
[32m+[m[32m        customCalculatorContainer.append(calculator);[m
[32m+[m[32m        customCalculatorContainer.classList.remove("visually-hidden");[m
[32m+[m[32m      } else if (calculatorName === "car-credit") {[m
[32m+[m[32m        calculator = document.createElement("car-credit-calculator");[m
[32m+[m[32m        customCalculatorContainer.append(calculator);[m
[32m+[m[32m        customCalculatorContainer.classList.remove("visually-hidden");[m
[32m+[m[32m      } else if (calculatorName === "consumer-credit") {[m
[32m+[m[32m        calculator = document.createElement("consumer-credit-calculator");[m
[32m+[m[32m        customCalculatorContainer.append(calculator);[m
[32m+[m[32m        customCalculatorContainer.classList.remove("visually-hidden");[m
[32m+[m[32m      } else {[m
[32m+[m[32m        offer.classList.add("visually-hidden");[m
[32m+[m[32m        request.classList.add("visually-hidden");[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  customElements.define("mortgage-calculator", MortgageCalculator);[m
[32m+[m[32m  customElements.define("consumer-credit-calculator", ConsumerCreditCalculator);[m
[32m+[m[32m  customElements.define("car-credit-calculator", CarCreditCalculator);[m
