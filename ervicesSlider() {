[32m+[m[32mexport default function calculator() {[m
[32m+[m[32m  const inputPrice = document.querySelector(".calculator__input_price");[m
[32m+[m[32m  const inputInitialFee = document.querySelector([m
[32m+[m[32m    ".calculator__input_initial-fee"[m
[32m+[m[32m  );[m
[32m+[m[32m  const rangeInitialFee = document.querySelector([m
[32m+[m[32m    ".calculator__input-range_initial-fee"[m
[32m+[m[32m  );[m
[32m+[m[32m  const outputInitialFee = document.querySelector([m
[32m+[m[32m    ".calculator__input-range_initial-fee-output"[m
[32m+[m[32m  );[m
[32m+[m[32m  const checkBoxMaternalCapital = document.querySelector([m
[32m+[m[32m    ".calculator__input-maternal-capital"[m
[32m+[m[32m  );[m
[32m+[m[32m  const inputTerm = document.querySelector(".calculator__input_term");[m
[32m+[m[32m  const rangeTerm = document.querySelector(".calculator__range_term");[m
[32m+[m[32m  const outputTerm = document.querySelector([m
[32m+[m[32m    ".calculator__range_term-output"[m
[32m+[m[32m  );[m
[32m+[m[32m  const maternalCapitalSum = 470000;[m
[32m+[m
[32m+[m[32m  let creditData = {[m
[32m+[m[32m    price: 0,[m
[32m+[m[32m    initialFee: 0,[m
[32m+[m[32m    initialFeePercantage: 0,[m
[32m+[m[32m    creditSum: 0,[m
[32m+[m[32m    term: 0,[m
[32m+[m[32m    useMaternalCapital: false,[m
[32m+[m[32m    interestRate: 0,[m
[32m+[m[32m    monthlyPayment: 0,[m
[32m+[m[32m    requiredIncome: 0,[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  inputPrice.addEventListener("input", () => calculateInitialFee());[m
[32m+[m[32m  inputInitialFee.addEventListener("input", () => updateInitialFee());[m
[32m+[m[32m  rangeInitialFee.addEventListener("input", () => updateInitialFeePercantage());[m
[32m+[m[32m  checkBoxMaternalCapital.addEventListener("change", () =>[m
[32m+[m[32m    changeMaternalCapital()[m
[32m+[m[32m  );[m
[32m+[m[32m  inputTerm.addEventListener("input", () => updateTerm(inputTerm.value));[m
[32m+[m[32m  rangeTerm.addEventListener("input", () => updateTerm(rangeTerm.value));[m
[32m+[m
[32m+[m[32m  function calculateInitialFee() {[m
[32m+[m[32m    creditData.price = inputPrice.value;[m
[32m+[m[32m    creditData.initialFee = inputPrice.value / 10;[m
[32m+[m[32m    creditData.initialFeePercantage =[m
[32m+[m[32m      (creditData.initialFee / creditData.price) * 100;[m
[32m+[m
[32m+[m[32m    inputInitialFee.value = creditData.initialFee;[m
[32m+[m[32m    rangeInitialFee.value = creditData.initialFeePercantage;[m
[32m+[m[32m    outputInitialFee.textContent = creditData.initialFeePercantage;[m
[32m+[m[32m    updateOffer();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function updateInitialFee() {[m
[32m+[m[32m    if (creditData.price) {[m
[32m+[m[32m      creditData.initialFee = inputInitialFee.value;[m
[32m+[m[32m      creditData.initialFeePercantage =[m
[32m+[m[32m        (creditData.initialFee / creditData.price) * 100;[m
[32m+[m
[32m+[m[32m      outputInitialFee.textContent = creditData.initialFeePercantage;[m
[32m+[m[32m      rangeInitialFee.value = creditData.initialFeePercantage;[m
[32m+[m[32m      updateOffer();[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function updateInitialFeePercantage() {[m
[32m+[m[32m    if (creditData.price) {[m
[32m+[m[32m      creditData.initialFeePercantage = rangeInitialFee.value;[m
[32m+[m[32m      creditData.initialFee =[m
[32m+[m[32m        (creditData.initialFeePercantage * creditData.price) / 100;[m
[32m+[m
[32m+[m[32m      outputInitialFee.textContent = creditData.initialFeePercantage;[m
[32m+[m[32m      inputInitialFee.value = creditData.initialFee;[m
[32m+[m[32m      updateOffer();[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function changeMaternalCapital() {[m
[32m+[m[32m    creditData.useMaternalCapital = checkBoxMaternalCapital.checked;[m
[32m+[m[32m    updateOffer();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function updateTerm(termValue) {[m
[32m+[m[32m    creditData.term = termValue;[m
[32m+[m
[32m+[m[32m    inputTerm.value = creditData.term;[m
[32m+[m[32m    rangeTerm.value = creditData.term;[m
[32m+[m[32m    updateOffer();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function updateOffer() {[m
[32m+[m[32m    creditData.creditSum =[m
[32m+[m[32m      creditData.price -[m
[32m+[m[32m      creditData.initialFee -[m
[32m+[m[32m      (creditData.useMaternalCapital ? maternalCapitalSum : 0);[m
[32m+[m
[32m+[m[32m    if (creditData.creditSum < 500000) {[m
[32m+[m[32m      showRefuse();[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (creditData.initialFeePercantage < 15) {[m
[32m+[m[32m      creditData.interestRate = 9.4;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      creditData.interestRate = 8.5;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    let monthlyInterestRate = creditData.interestRate / 12 / 100;[m
[32m+[m
[32m+[m[32m    creditData.monthlyPayment = Math.ceil([m
[32m+[m[32m      creditData.creditSum *[m
[32m+[m[32m        (monthlyInterestRate +[m
[32m+[m[32m          monthlyInterestRate /[m
[32m+[m[32m            ((1 + monthlyInterestRate) ** (creditData.term * 12) - 1))[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    creditData.requiredIncome = Math.ceil([m
[32m+[m[32m      (creditData.monthlyPayment * 100) / 45[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    showOffer(creditData);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function showRefuse() {}[m
[32m+[m[32m}[m
