[32m+[m[32mexport default function makeRequest(purpose, price, initialFee, term) {[m
[32m+[m[32m  const request = document.querySelector(".request");[m
[32m+[m[32m  const requestForm = document.querySelector(".request__form");[m
[32m+[m[32m  const inputRequestNumber = request.querySelector([m
[32m+[m[32m    ".request__input_request-number"[m
[32m+[m[32m  );[m
[32m+[m[32m  const inputPurpose = request.querySelector(".request__input_purpose");[m
[32m+[m[32m  const inputPrice = request.querySelector(".request__input_price");[m
[32m+[m[32m  const inputInitialFee = request.querySelector(".request__input_initial-fee");[m
[32m+[m[32m  const inputTerm = request.querySelector(".request__input_term");[m
[32m+[m[32m  const labelPrice = request.querySelector(".request__label_price");[m
[32m+[m[32m  const userInfoInputs = request.querySelectorAll(".request__input");[m
[32m+[m[32m  const inputFullname = request.querySelector(".request__input_fullname");[m
[32m+[m[32m  const inputTelephone = request.querySelector(".request__input_phone-number");[m
[32m+[m[32m  const inputEmail = request.querySelector(".request__input_email");[m
[32m+[m
[32m+[m[32m  inputFullname.focus();[m
[32m+[m[32m  userInfoInputs.forEach((item) => {[m
[32m+[m[32m    item.addEventListener("change", () => checkEmpty(item));[m
[32m+[m[32m  });[m
[32m+[m[32m  requestForm.addEventListener("submit", submitForm);[m
[32m+[m
[32m+[m[32m  function checkEmpty(input) {[m
[32m+[m[32m    input.nextElementSibling;[m
[32m+[m[32m    if (!input.value.length) {[m
[32m+[m[32m      input.nextElementSibling.classList.remove("request__label_moved");[m
[32m+[m[32m    } else {[m
[32m+[m[32m      input.nextElementSibling.classList.add("request__label_moved");[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (localStorage.getItem("user-data") === null) {[m
[32m+[m[32m    localStorage.setItem("user-data", []);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  //slice(-4) добавляет числу лишние нули[m
[32m+[m[32m  inputRequestNumber.value = `№ ${([m
[32m+[m[32m    "0000" +[m
[32m+[m[32m    (+localStorage.getItem("requestsNumber") + 1)[m
[32m+[m[32m  ).slice(-4)}`;[m
[32m+[m
[32m+[m[32m  function submitForm() {[m
[32m+[m[32m    event.preventDefault();[m
[32m+[m
[32m+[m[32m    if (!isFormVaild()) {[m
[32m+[m[32m      request.classList.add("request_invalid");[m
[32m+[m
[32m+[m[32m      request.addEventListener("animationend", removeAnimation);[m
[32m+[m
[32m+[m[32m      function removeAnimation() {[m
[32m+[m[32m        request.classList.remove("request_invalid");[m
[32m+[m[32m        request.removeEventListener("animationend", removeAnimation);[m
[32m+[m[32m      }[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    localStorage.setItem([m
[32m+[m[32m      "requestsNumber",[m
[32m+[m[32m      +localStorage.getItem("requestsNumber") + 1[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    localStorage.getItem("user-data");[m
[32m+[m
[32m+[m[32m    localStorage.setItem("user-data", [[m
[32m+[m[32m      //...localStorage.getItem("user-data"),[m
[32m+[m[32m      {[m
[32m+[m[32m        fullName: inputFullname.value,[m
[32m+[m[32m        telephone: inputTelephone.value,[m
[32m+[m[32m        email: inputEmail.value,[m
[32m+[m[32m        number: +localStorage.getItem("requestsNumber") + 1,[m
[32m+[m[32m      },[m
[32m+[m[32m    ]);[m
[32m+[m
[32m+[m[32m    inputFullname.value = "";[m
[32m+[m[32m    inputTelephone.value = "";[m
[32m+[m[32m    inputEmail.value = "";[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function isFormVaild() {[m
[32m+[m[32m    if ([m
[32m+[m[32m      inputFullname.value.length &&[m
[32m+[m[32m      inputTelephone.value.length &&[m
[32m+[m[32m      inputEmail.value.length[m
[32m+[m[32m    ) {[m
[32m+[m[32m      return true;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  request.classList.remove("visually-hidden");[m
[32m+[m
[32m+[m[32m  if (purpose === "mortgage") {[m
[32m+[m[32m    inputPurpose.value = "Ипотека";[m
[32m+[m[32m    labelPrice.textContent = "Стоимость недвижимости";[m
[32m+[m[32m  } else if (purpose === "car-credit") {[m
[32m+[m[32m    inputPurpose.value = "Автокредит";[m
[32m+[m[32m    labelPrice.textContent = "Стоимость автомобиля";[m
[32m+[m[32m  } else {[m
[32m+[m[32m    inputPurpose.value = "Потребительский кредит";[m
[32m+[m[32m    labelPrice.textContent = "Сумма кредита";[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  inputPrice.value = price;[m
[32m+[m[32m  inputInitialFee.value = initialFee;[m
[32m+[m[32m  inputTerm.value = term;[m
[32m+[m[32m}[m
